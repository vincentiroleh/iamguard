# AWS Configuration
AWS_REGION=us-east-1
AWS_PROFILE=default

# IAM Scanner Configuration
IAM_INACTIVE_DAYS_THRESHOLD=30
IAM_ACCESS_KEY_AGE_THRESHOLD=90
IAM_MAX_CONCURRENT_REQUESTS=10
IAM_RETRY_ATTEMPTS=3
IAM_TIMEOUT=30000

# Report Configuration
IAM_AUTO_OPEN_REPORTS=true
IAM_REPORT_FORMATS=json,html,csv

# Compliance Framework
IAM_COMPLIANCE_FRAMEWORK=CIS
IAM_ENABLE_COMPLIANCE_CHECKS=true